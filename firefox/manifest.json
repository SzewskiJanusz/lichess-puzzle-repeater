{

  "manifest_version": 2,
  "name": "Lichess puzzle repeater",
  "version": "1.0",

  "description": "Easier analysis access of wrongly solved puzzles on PuzzleRush",

  "icons": {
  },

  "content_scripts": [
    {
      "matches": ["*://lichess.org/racer/*"],
      "js": ["puzzle-repeater.js"]
    },
	{
      "matches": ["*://lichess.org/training/*"],
      "js": ["training-board.js"]
    }
  ]

}
